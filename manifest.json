{
    "display_information": {
        "name": "FRACTRAN",
        "description": "Executes Fractran",
        "background_color": "#2c2d30"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "FRACTRAN",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/fractran",
                "description": "Executes a fractran program",
                "usage_hint": "[fractions] [input]",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "commands"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": true,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}